<!-- Application -->
<div class="todo-container">
    <h1>Bootstrap Todo App</h1>

  <div class="add-todo-co-8">
      <input type="text" class="form-control" placeholder="Have Todo">
      <button class="btn btn-primary">Add</button>
    </div>
    <div class="col-md-4">

    </div>
  </div>
        
  <ul class="list-group">
   
   
  </ul>
</div>

<!-- templates -->
<script type="text/x-template">
 <li class="list-group-item row {{#if completed}}disabled{{/if}}">
        <div class="col-md-1"><input type="checkbox" {{#if completed}}checked{{/if}}></div>
        <div class="col-md-10">{{title}}</div>
        <div class="col-md-1"><button type="button" class="close" aria-label="Close"><span aria-hidden="true">&times;</span></button>
      </div>
    </li>
</script>
